{
  "name": "JeAP Glow",
  "type": "dark",
  "colors": {
    "editor.background": "#2E2E2E",
    "editorCursor.foreground": "#FFFFFF",
    "editor.foreground": "#D6D6D6",
    "editorWhitespace.foreground": "#404040",
    "editor.lineHighlightBackground": "#333435",
    "editor.selectionBackground": "#5A647E",
    "tab.activeBorder": "#B05279"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#a4a2a2"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#E5B567"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#9E86C8"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#9E86C8"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other", "constant.language"],
      "settings": {
        "foreground": "#9E86C8"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#e7eaf6"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#B05279"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#B05279"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6C99BB"
      }
    },
    {
      "name": "Storage Type Class",
      "scope": [
        "storage.type.class.js",
        "storage.modifier.js",
        "variable.language.super.js",
        "variable.language.this.js",
        "storage.type.class.php",
        "variable.language.this.php"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#C5A5C2"
      }
    },
    {
      "name": "Object Call",
      "scope": [
        "punctuation.definition.block.js",
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.accessor.js",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#88C6BE"
      }
    },
    {
      "name": "Support Class Component",
      "scope": ["support.class.component.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6C99BB"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#F0688A"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FAC863"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "",
        "foreground": "#B4C973"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#E87D3E",
        "fontStyle": " italic "
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "#f77180"
      }
    },
    {
      "name": "Tag Definitions",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.tag.begin.js",
        "punctuation.definition.tag.end.js"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#badfda"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "#B4C973"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "#6C99BB"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#6C99BB",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#EDF0F1"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#EDF0F1"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Support Function",
      "scope": ["support.function", "support.variable.property.js"],
      "settings": {
        "fontStyle": "",
        "foreground": "#C5A5C2"
      }
    },
    {
      "name": "Entity Name Function",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#B4C973"
      }
    },
    {
      "name": "Brace Round",
      "scope": [
        "meta.brace.round",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end"
      ],
      "settings": {
        "foreground": "#F0E76C"
      }
    },
    {
      "name": "Brace Curly",
      "scope": "meta.brace.curly",
      "settings": {
        "foreground": "#D7BFEB"
      }
    },
    {
      "name": "Brace Square",
      "scope": [
        "meta.brace.square",
        "punctuation.definition.binding-pattern.array.js"
      ],
      "settings": {
        "foreground": "#a7bae9"
      }
    },
    {
      "name": "Variable Parameter Function",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#E87D3E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Support Variable Property",
      "scope": "meta.delimiter.method.period",
      "settings": {
        "foreground": "#B05279"
      }
    },
    {
      "name": "Variable Language",
      "scope": "variable.language",
      "settings": {
        "foreground": "#A57EB5"
      }
    },
    {
      "name": "Markdown Image",
      "scope": ["meta.link.inline.markdown", "meta.image.inline.markdown"],
      "settings": {
        "foreground": "#B4C973"
      }
    },
    {
      "name": "GitGutter inserted",
      "scope": "markup.inserted.git_gutter",
      "settings": {
        "foreground": "#B4C973"
      }
    },
    {
      "name": "GitGutter changed",
      "scope": "markup.changed.git_gutter",
      "settings": {
        "foreground": "#CC7732"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6C99BB"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#6C99BB"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B4C973"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes property value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#C5A5C2"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#B4C973"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#6C99BB"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6C99BB"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
